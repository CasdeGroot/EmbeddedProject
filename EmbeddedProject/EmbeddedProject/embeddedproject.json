{
  "vision": {
    "img_folder": "../Captured_Image",
    "debug": true
  },
  "drivers": {
    "blacklight": {
      "driver": "drivers.sensordriver.SensorDriver",
      "com_port": "COM10",
      "baud_rate": 115200,
      "timeout": 5,
      "writeTimeout": 3,
      "handler": "drivers.handlers.BlacklightHandler",
      "enable": true
    },
    "roughness": {
      "driver": "drivers.sensordriver.SensorDriver",
      "com_port": "COM4",
      "baud_rate": 115200,
      "timeout": 1,
      "writeTimeout": 1,
      "handler": "drivers.handlers.RoughnessHandler",
      "enable": false
    },
    "controller": {
      "driver": "drivers.networkdriver.NetworkDriver",
      "send_ip": "192.168.1.2",
      "send_port": "1337",
      "listen_ip": "192.168.1.3",
      "listen_port": "1338",
      "handler": "drivers.handlers.ControllerHandler",
      "enable": true
    }
  }
}